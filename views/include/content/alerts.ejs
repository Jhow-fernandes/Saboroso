<!-- SE DE ALGUM ERRO IRA APARECER UMA MENSSAGEM DE ERRO -->
<% if(typeof error === "string") {%>
<div class="alert alert-danger">
    <%= error %>
</div>
<% } %>

<!-- SE DE TUDO CERTO IRA APARECER UMA MENSSAGEM DE SUCESSO -->
<% if(typeof success === "string") {%>
<div class="alert alert-success">
    <%= success %>
</div>
<% } %>